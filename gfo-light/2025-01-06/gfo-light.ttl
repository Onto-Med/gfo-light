###  https://w3id.org/gfo-light/after
<https://w3id.org/gfo-light/after> rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf <https://w3id.org/gfo-light/temporallyRelatedWith> ;
                                   owl:inverseOf <https://w3id.org/gfo-light/before> ;
                                   rdfs:label "after"@en .


###  https://w3id.org/gfo-light/attributiveOf
<https://w3id.org/gfo-light/attributiveOf> rdf:type owl:ObjectProperty ;
                                           owl:inverseOf <https://w3id.org/gfo-light/hasAttributive> ;
                                           rdfs:domain <https://w3id.org/gfo-light/Attributive> ;
                                           rdfs:range <https://w3id.org/gfo-light/Individual> ;
                                           rdfs:label "attributive of"@en .


###  https://w3id.org/gfo-light/before
<https://w3id.org/gfo-light/before> rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf <https://w3id.org/gfo-light/temporallyRelatedWith> ;
                                    rdfs:label "before"@en ;
                                    <http://www.w3.org/2004/02/skos/core#definition> "X before Y: Xf < Ys"@en .


###  https://w3id.org/gfo-light/boundaryOf
<https://w3id.org/gfo-light/boundaryOf> rdf:type owl:ObjectProperty ;
                                        owl:inverseOf <https://w3id.org/gfo-light/hasBoundary> ;
                                        rdfs:domain <https://w3id.org/gfo-light/ImmaterialContinuant> ;
                                        rdfs:range [ rdf:type owl:Class ;
                                                     owl:unionOf ( <https://w3id.org/gfo-light/Line>
                                                                   <https://w3id.org/gfo-light/MaterialObject>
                                                                   <https://w3id.org/gfo-light/Surface>
                                                                 )
                                                   ] ;
                                        rdfs:comment "Immaterial continuants can be considered as (outer or inner) boundaries (boundaryOf). Note that surfaces are boundaries of material continuants, lines are boundaries of surfaces and points are boundaries of lines."@en ;
                                        rdfs:label "boundary of"@en .


###  https://w3id.org/gfo-light/causedBy
<https://w3id.org/gfo-light/causedBy> rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf <https://w3id.org/gfo-light/resultsFrom> ;
                                      owl:inverseOf <https://w3id.org/gfo-light/causes> ;
                                      rdfs:label "caused by"@en .


###  https://w3id.org/gfo-light/causes
<https://w3id.org/gfo-light/causes> rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf <https://w3id.org/gfo-light/leadsTo> ;
                                    rdfs:label "causes"@en .


###  https://w3id.org/gfo-light/containsRole
<https://w3id.org/gfo-light/containsRole> rdf:type owl:ObjectProperty ;
                                          owl:inverseOf <https://w3id.org/gfo-light/roleIn> ;
                                          rdfs:domain [ rdf:type owl:Class ;
                                                        owl:unionOf ( <https://w3id.org/gfo-light/Continuant>
                                                                      <https://w3id.org/gfo-light/Process>
                                                                      <https://w3id.org/gfo-light/Relator>
                                                                      <https://w3id.org/gfo-light/Situation>
                                                                    )
                                                      ] ;
                                          rdfs:range <https://w3id.org/gfo-light/Role> ;
                                          rdfs:label "contains role"@en .


###  https://w3id.org/gfo-light/during
<https://w3id.org/gfo-light/during> rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf <https://w3id.org/gfo-light/temporalPartOf> ;
                                    owl:inverseOf <https://w3id.org/gfo-light/temporallyContains> ;
                                    rdfs:label "during"@en ;
                                    <http://www.w3.org/2004/02/skos/core#definition> "X during Y: Xs > Ys and Xf < Yf"@en .


