                                    rdfs:range [ rdf:type owl:Class ;
                                                 owl:unionOf ( <https://w3id.org/gfo-light/Continuant>
                                                               <https://w3id.org/gfo-light/Process>
                                                               <https://w3id.org/gfo-light/Relator>
                                                               <https://w3id.org/gfo-light/Situation>
                                                             )
                                               ] ;
                                    rdfs:comment "Roles are assigned to the corresponding context entity (continuant, process, situation, relator) using object properties roleIn or containsRole."@en ;
                                    rdfs:label "role in"@en .


###  https://w3id.org/gfo-light/situationPartOf
<https://w3id.org/gfo-light/situationPartOf> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <https://w3id.org/gfo-light/partOf> ;
                                             rdfs:domain [ rdf:type owl:Class ;
                                                           owl:unionOf ( <https://w3id.org/gfo-light/Attributive>
                                                                         <https://w3id.org/gfo-light/Situation>
                                                                       )
                                                         ] ;
                                             rdfs:range <https://w3id.org/gfo-light/Situation> ;
                                             rdfs:label "situation part of"@en .


###  https://w3id.org/gfo-light/spatiallyEndsOn
<https://w3id.org/gfo-light/spatiallyEndsOn> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <https://w3id.org/gfo-light/spatiallyPassesThrough> ;
                                             rdfs:label "spatially ends on"@en .


###  https://w3id.org/gfo-light/spatiallyPassesThrough
<https://w3id.org/gfo-light/spatiallyPassesThrough> rdf:type owl:ObjectProperty ;
                                                    rdfs:domain [ rdf:type owl:Class ;
                                                                  owl:unionOf ( <https://w3id.org/gfo-light/Line>
                                                                                <https://w3id.org/gfo-light/Surface>
                                                                              )
                                                                ] ;
                                                    rdfs:range <https://w3id.org/gfo-light/Continuant> ;
                                                    rdfs:label "spatially passes through"@en .


###  https://w3id.org/gfo-light/spatiallyStartsOn
<https://w3id.org/gfo-light/spatiallyStartsOn> rdf:type owl:ObjectProperty ;
                                               rdfs:subPropertyOf <https://w3id.org/gfo-light/spatiallyPassesThrough> ;
                                               rdfs:label "spatially starts on"@en .


###  https://w3id.org/gfo-light/temporalPartOf
<https://w3id.org/gfo-light/temporalPartOf> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf <https://w3id.org/gfo-light/temporallyRelatedWith> ;
                                            rdfs:label "temporal part of"@en ;
                                            <http://www.w3.org/2004/02/skos/core#definition> "X temporal part of Y: X starts Y or X finishes Y or X during Y"@en .


###  https://w3id.org/gfo-light/temporallyContains
<https://w3id.org/gfo-light/temporallyContains> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <https://w3id.org/gfo-light/hasTemporalPart> ;
                                                rdfs:label "temporally contains"@en .


###  https://w3id.org/gfo-light/temporallyEqual
<https://w3id.org/gfo-light/temporallyEqual> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <https://w3id.org/gfo-light/temporallyRelatedWith> ;
                                             rdfs:label "temporally equal"@en ;
                                             <http://www.w3.org/2004/02/skos/core#definition> "X equal Y: Xs = Ys and Xf = Yf"@en .


###  https://w3id.org/gfo-light/temporallyFinishedBy
<https://w3id.org/gfo-light/temporallyFinishedBy> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <https://w3id.org/gfo-light/hasTemporalPart> ;
                                                  owl:inverseOf <https://w3id.org/gfo-light/temporallyFinishes> ;
                                                  rdfs:label "temporally finished by"@en .


###  https://w3id.org/gfo-light/temporallyFinishes
<https://w3id.org/gfo-light/temporallyFinishes> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <https://w3id.org/gfo-light/temporalPartOf> ;
                                                rdfs:label "temporally finishes"@en ;
                                                <http://www.w3.org/2004/02/skos/core#definition> "X finishes Y: Xs > Ys and Xf = Yf"@en .


###  https://w3id.org/gfo-light/temporallyMeets
<https://w3id.org/gfo-light/temporallyMeets> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <https://w3id.org/gfo-light/temporallyRelatedWith> ;
                                             owl:inverseOf <https://w3id.org/gfo-light/temporallyMetBy> ;
                                             rdfs:label "temporally meets"@en ;
                                             <http://www.w3.org/2004/02/skos/core#definition> "X meets Y: Xf = Ys"@en .


###  https://w3id.org/gfo-light/temporallyMetBy
<https://w3id.org/gfo-light/temporallyMetBy> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <https://w3id.org/gfo-light/temporallyRelatedWith> ;
                                             rdfs:label "temporally met by"@en .


###  https://w3id.org/gfo-light/temporallyOverlappedBy
<https://w3id.org/gfo-light/temporallyOverlappedBy> rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf <https://w3id.org/gfo-light/temporallyRelatedWith> ;
                                                    owl:inverseOf <https://w3id.org/gfo-light/temporallyOverlaps> ;
                                                    rdfs:label "temporally overlapped by"@en .


###  https://w3id.org/gfo-light/temporallyOverlaps
<https://w3id.org/gfo-light/temporallyOverlaps> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <https://w3id.org/gfo-light/temporallyRelatedWith> ;
                                                rdfs:label "temporally overlaps"@en ;
                                                <http://www.w3.org/2004/02/skos/core#definition> "X overlaps Y: Xs < Ys and Xf > Ys and Xf < Yf"@en .


###  https://w3id.org/gfo-light/temporallyRelatedWith
<https://w3id.org/gfo-light/temporallyRelatedWith> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <https://w3id.org/gfo-light/Individual> ;
                                                   rdfs:range <https://w3id.org/gfo-light/Individual> ;
                                                   rdfs:comment "If a temporal relation is applied to other concrete individuals as time intervals (attributive, continuant, process, situation), it is propagated to their existence times (hasTime)."@en ,
                                                                "In the definitions of the relations, the variables Xs, Xf, Ys and Yf represent the start (s) and finish (f) of the time intervals X and Y."@en ,
                                                                "Some temporal relations can also be applied to points in time (before/after, equal) or a point in time and a time interval (before, starts, finishes, during or their inverse relations)."@en ;
                                                   rdfs:label "temporally related with"@en ;
                                                   <http://www.w3.org/2004/02/skos/core#definition> "Temporal relations between time intervals based on Allen, J. F. (1983). Maintaining knowledge about temporal intervals. Communications of the ACM, 26(11), 832-843."@en .


###  https://w3id.org/gfo-light/temporallyStartedBy
<https://w3id.org/gfo-light/temporallyStartedBy> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf <https://w3id.org/gfo-light/hasTemporalPart> ;
                                                 owl:inverseOf <https://w3id.org/gfo-light/temporallyStarts> ;
                                                 rdfs:label "temporally started by"@en .


###  https://w3id.org/gfo-light/temporallyStarts
<https://w3id.org/gfo-light/temporallyStarts> rdf:type owl:ObjectProperty ;
                                              rdfs:subPropertyOf <https://w3id.org/gfo-light/temporalPartOf> ;
                                              rdfs:label "temporally starts"@en ;
                                              <http://www.w3.org/2004/02/skos/core#definition> "X starts Y: Xs = Ys and Xf < Yf"@en .


###  https://w3id.org/gfo-light/timeOf
<https://w3id.org/gfo-light/timeOf> rdf:type owl:ObjectProperty ;
                                    rdfs:domain <https://w3id.org/gfo-light/TimeEntity> ;
                                    rdfs:range <https://w3id.org/gfo-light/ConcreteIndividual> ;
                                    rdfs:label "time of"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/gfo-light/afterDateTimeValue
<https://w3id.org/gfo-light/afterDateTimeValue> rdf:type owl:DatatypeProperty ;
                                                rdfs:subPropertyOf <https://w3id.org/gfo-light/dateTimeValue> ;
                                                rdfs:comment "Shortcut for 'after'"@en ;
                                                rdfs:label "after date time value"@en .


###  https://w3id.org/gfo-light/beforeDateTimeValue
<https://w3id.org/gfo-light/beforeDateTimeValue> rdf:type owl:DatatypeProperty ;
                                                 rdfs:subPropertyOf <https://w3id.org/gfo-light/dateTimeValue> ;
                                                 rdfs:comment "Shortcut for 'before'"@en ;
                                                 rdfs:label "before date time value"@en .


###  https://w3id.org/gfo-light/booleanValue
<https://w3id.org/gfo-light/booleanValue> rdf:type owl:DatatypeProperty ;
                                          rdfs:domain <https://w3id.org/gfo-light/Attributive> ;
                                          rdfs:range xsd:boolean ;
                                          rdfs:label "boolean value"@en .


###  https://w3id.org/gfo-light/containsDateTimeValue
<https://w3id.org/gfo-light/containsDateTimeValue> rdf:type owl:DatatypeProperty ;
                                                   rdfs:subPropertyOf <https://w3id.org/gfo-light/hasTemporalPartDateTimeValue> ;
                                                   rdfs:comment "Shortcut for 'temporallyContains'"@en ;
                                                   rdfs:label "contains date time value"@en .


###  https://w3id.org/gfo-light/dateTimeValue
<https://w3id.org/gfo-light/dateTimeValue> rdf:type owl:DatatypeProperty ;
                                           rdfs:domain <https://w3id.org/gfo-light/Individual> ;
                                           rdfs:range xsd:dateTime ;
                                           rdfs:label "date time value"@en .


###  https://w3id.org/gfo-light/decimalValue
<https://w3id.org/gfo-light/decimalValue> rdf:type owl:DatatypeProperty ;
                                          rdfs:domain <https://w3id.org/gfo-light/Attributive> ;
                                          rdfs:range xsd:decimal ;
                                          rdfs:label "decimal value"@en .


###  https://w3id.org/gfo-light/finishedByDateTimeValue
<https://w3id.org/gfo-light/finishedByDateTimeValue> rdf:type owl:DatatypeProperty ;
                                                     rdfs:subPropertyOf <https://w3id.org/gfo-light/hasTemporalPartDateTimeValue> ;
                                                     rdfs:comment "Shortcut for 'temporallyFinishedBy'"@en ;
                                                     rdfs:label "finished by date time value"@en .


###  https://w3id.org/gfo-light/hasTemporalPartDateTimeValue
<https://w3id.org/gfo-light/hasTemporalPartDateTimeValue> rdf:type owl:DatatypeProperty ;
                                                          rdfs:subPropertyOf <https://w3id.org/gfo-light/dateTimeValue> ;
                                                          rdfs:comment "Shortcut for 'hasTemporalPart'"@en ;
                                                          rdfs:label "has temporal part date time value"@en .


###  https://w3id.org/gfo-light/startedByDateTimeValue
<https://w3id.org/gfo-light/startedByDateTimeValue> rdf:type owl:DatatypeProperty ;
                                                    rdfs:subPropertyOf <https://w3id.org/gfo-light/hasTemporalPartDateTimeValue> ;
                                                    rdfs:comment "Shortcut for 'temporallyStartedBy'"@en ;
                                                    rdfs:label "started by date time value"@en .


###  https://w3id.org/gfo-light/stringValue
<https://w3id.org/gfo-light/stringValue> rdf:type owl:DatatypeProperty ;
                                         rdfs:domain <https://w3id.org/gfo-light/Attributive> ;
                                         rdfs:range xsd:string ;
                                         rdfs:label "string value"@en .


###  https://w3id.org/gfo-light/unit
<https://w3id.org/gfo-light/unit> rdf:type owl:DatatypeProperty ;
                                  rdfs:domain <https://w3id.org/gfo-light/Attributive> ;
                                  rdfs:range xsd:string ;
                                  rdfs:label "unit"@en .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/gfo-light/Attributive
<https://w3id.org/gfo-light/Attributive> rdf:type owl:Class ;
                                         rdfs:subClassOf <https://w3id.org/gfo-light/ConcreteIndividual> ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <https://w3id.org/gfo-light/attributiveOf> ;
                                                           owl:someValuesFrom <https://w3id.org/gfo-light/Individual>
                                                         ] ;
                                         rdfs:comment "Qualities (attributes, traits, characteristics, etc.) of concrete individuals, relations (relators) between them and roles that objects can play in different contexts are subsumed under the category Attributive."@en ;
                                         rdfs:label "Attributive"@en ;
                                         <http://www.w3.org/2004/02/skos/core#definition> "Attributives are individuals that depend on other individuals by some kind of dependency relation."@en .


###  https://w3id.org/gfo-light/Category
<https://w3id.org/gfo-light/Category> rdf:type owl:Class ;
                                      rdfs:subClassOf <https://w3id.org/gfo-light/Entity> ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty <https://w3id.org/gfo-light/instantiatedBy> ;
                                                        owl:allValuesFrom [ rdf:type owl:Class ;
                                                                            owl:unionOf ( <https://w3id.org/gfo-light/Category>
                                                                                          <https://w3id.org/gfo-light/Individual>
                                                                                        )
                                                                          ]
                                                      ] ;
                                      rdfs:label "Category"@en ;
                                      <http://www.w3.org/2004/02/skos/core#altLabel> "Class"@en ,
                                                                                     "Kind"@en ,
                                                                                     "Type"@en ;
                                      <http://www.w3.org/2004/02/skos/core#definition> "The notion of category covers all abstract entities that can be instantiated by or are predicated of other entities."@en .


###  https://w3id.org/gfo-light/Chronoid
<https://w3id.org/gfo-light/Chronoid> rdf:type owl:Class ;
                                      rdfs:subClassOf <https://w3id.org/gfo-light/TimeEntity> ,
                                                      [ rdf:type owl:Class ;
                                                        owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                               owl:onProperty <https://w3id.org/gfo-light/temporallyFinishedBy> ;
                                                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                               owl:onClass <https://w3id.org/gfo-light/TimeBoundary>
                                                                                             ]
                                                                                             [ rdf:type owl:Restriction ;
                                                                                               owl:onProperty <https://w3id.org/gfo-light/temporallyStartedBy> ;
                                                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                               owl:onClass <https://w3id.org/gfo-light/TimeBoundary>
                                                                                             ]
                                                                                           ) ;
                                                                        rdf:type owl:Class
                                                                      ]
                                                                      [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                               owl:onProperty <https://w3id.org/gfo-light/finishedByDateTimeValue> ;
                                                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                               owl:onDataRange xsd:dateTime
                                                                                             ]
                                                                                             [ rdf:type owl:Restriction ;
                                                                                               owl:onProperty <https://w3id.org/gfo-light/startedByDateTimeValue> ;
                                                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                               owl:onDataRange xsd:dateTime
                                                                                             ]
                                                                                           ) ;
                                                                        rdf:type owl:Class
                                                                      ]
                                                                    )
                                                      ] ;
                                      rdfs:label "Chronoid"@en ;
                                      <http://www.w3.org/2004/02/skos/core#altLabel> "Time interval"@en ,
                                                                                     "Time period"@en ;
                                      <http://www.w3.org/2004/02/skos/core#definition> "Chronoids are real-valued time intervals with endpoints."@en ;
                                      <http://www.w3.org/2004/02/skos/core#example> "The time (extension) of a football match (from the start point in time to the end point in time)"@en .


###  https://w3id.org/gfo-light/ConcreteIndividual
<https://w3id.org/gfo-light/ConcreteIndividual> rdf:type owl:Class ;
                                                rdfs:subClassOf <https://w3id.org/gfo-light/Individual> ,
                                                                [ rdf:type owl:Class ;
                                                                  owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty <https://w3id.org/gfo-light/finishedByDateTimeValue> ;
                                                                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                                         owl:onDataRange xsd:dateTime
                                                                                                       ]
                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty <https://w3id.org/gfo-light/startedByDateTimeValue> ;
                                                                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                                         owl:onDataRange xsd:dateTime
                                                                                                       ]
                                                                                                     ) ;
                                                                                  rdf:type owl:Class
                                                                                ]
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <https://w3id.org/gfo-light/hasTime> ;
                                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                  owl:onClass <https://w3id.org/gfo-light/TimeEntity>
                                                                                ]
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <https://w3id.org/gfo-light/dateTimeValue> ;
                                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                  owl:onDataRange xsd:dateTime
                                                                                ]
                                                                              )
                                                                ] ;
                                                rdfs:label "Concrete individual"@en ;
                                                <http://www.w3.org/2004/02/skos/core#definition> "Concrete individuals are entities that have an immediate relation to time or to space-time. They exist in time."@en .


###  https://w3id.org/gfo-light/Continuant
<https://w3id.org/gfo-light/Continuant> rdf:type owl:Class ;
                                        rdfs:subClassOf <https://w3id.org/gfo-light/ConcreteIndividual> ,
                                                        [ rdf:type owl:Class ;
                                                          owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                 owl:onProperty <https://w3id.org/gfo-light/finishedByDateTimeValue> ;
                                                                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                                 owl:onDataRange xsd:dateTime
                                                                                               ]
                                                                                               [ rdf:type owl:Restriction ;
                                                                                                 owl:onProperty <https://w3id.org/gfo-light/startedByDateTimeValue> ;
                                                                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                                 owl:onDataRange xsd:dateTime
                                                                                               ]
                                                                                             ) ;
                                                                          rdf:type owl:Class
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <https://w3id.org/gfo-light/hasTime> ;
                                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                          owl:onClass <https://w3id.org/gfo-light/Chronoid>
                                                                        ]
                                                                      )
                                                        ] ;
                                        rdfs:comment "Material three-dimensional objects, their aggregates (groups) and parts as well as immaterial two-, one- and zero-dimensional entities (i.e., surfaces, lines and points) are subsumed under the category Continuant."@en ;
                                        rdfs:label "Continuant"@en ;
                                        <http://www.w3.org/2004/02/skos/core#altLabel> "Object"@en ;
                                        <http://www.w3.org/2004/02/skos/core#definition> "Continuants are concrete individuals that persist through time and are wholly present at every point of their existence time (lifetime)."@en .


###  https://w3id.org/gfo-light/ContinuantPartRole
<https://w3id.org/gfo-light/ContinuantPartRole> rdf:type owl:Class ;
                                                rdfs:subClassOf <https://w3id.org/gfo-light/Role> ,
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <https://w3id.org/gfo-light/roleIn> ;
                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onClass <https://w3id.org/gfo-light/Continuant>
                                                                ] ;
                                                rdfs:label "Continuant part role"@en ;
                                                <http://www.w3.org/2004/02/skos/core#altLabel> "Object part role"@en ;
                                                <http://www.w3.org/2004/02/skos/core#definition> "A continuant part role is the role that a continuant part plays in the whole continuant."@en ;
                                                <http://www.w3.org/2004/02/skos/core#example> "John is part/member of the football team and plays the role of midfielder."@en ,
                                                                                              "The processor is part of the computer and has the task/function/role to process commands."@en .


###  https://w3id.org/gfo-light/Entity
<https://w3id.org/gfo-light/Entity> rdf:type owl:Class ;
                                    rdfs:label "Entity"@en ;
                                    <http://www.w3.org/2004/02/skos/core#definition> "Everything that exists is called an entity."@en .


###  https://w3id.org/gfo-light/ImmaterialContinuant
<https://w3id.org/gfo-light/ImmaterialContinuant> rdf:type owl:Class ;
                                                  rdfs:subClassOf <https://w3id.org/gfo-light/Continuant> ,
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <https://w3id.org/gfo-light/hasOwner> ;
                                                                    owl:someValuesFrom <https://w3id.org/gfo-light/MaterialContinuant>
                                                                  ] ;
                                                  rdfs:comment "Immaterial continuants can border material continuants (natural boundaries) or be located inside them (inner boundaries). Boundaries of material continuants are surfaces, boundaries of surfaces are lines and boundaries of lines are points."@en ;
                                                  rdfs:label "Immaterial continuant"@en ;
                                                  <http://www.w3.org/2004/02/skos/core#altLabel> "Object boundary"@en ;
                                                  <http://www.w3.org/2004/02/skos/core#definition> "Immaterial continuants are 0-, 1-, or 2-dimensional immaterial (visible or imaginary) entities (i.e., points, lines and surfaces) that are located on material continuants or in them."@en .


###  https://w3id.org/gfo-light/Individual
<https://w3id.org/gfo-light/Individual> rdf:type owl:Class ;
                                        rdfs:subClassOf <https://w3id.org/gfo-light/Entity> ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty <https://w3id.org/gfo-light/instanceOf> ;
                                                          owl:someValuesFrom <https://w3id.org/gfo-light/Category>
                                                        ] ;
                                        rdfs:label "Individual"@en ;
                                        <http://www.w3.org/2004/02/skos/core#definition> "Individuals are entities which cannot be further instantiated."@en .


###  https://w3id.org/gfo-light/Line
<https://w3id.org/gfo-light/Line> rdf:type owl:Class ;
                                  rdfs:subClassOf <https://w3id.org/gfo-light/ImmaterialContinuant> ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <https://w3id.org/gfo-light/boundaryOf> ;
                                                    owl:allValuesFrom <https://w3id.org/gfo-light/Surface>
                                                  ] ;
                                  rdfs:comment "Lines are not necessarily straight. They can be, for example, curves or broken lines that pass through various points and form a corner, a triangle or a polygon."@en ;
                                  rdfs:label "Line"@en ;
                                  <http://www.w3.org/2004/02/skos/core#definition> "A line is a 1-dimensional immaterial continuant."@en ;
                                  <http://www.w3.org/2004/02/skos/core#example> "Boundary line of the football field"@en ,
                                                                                "Longitudinal axis of the human skeleton"@en .


###  https://w3id.org/gfo-light/MaterialAggregate
<https://w3id.org/gfo-light/MaterialAggregate> rdf:type owl:Class ;
                                               rdfs:subClassOf <https://w3id.org/gfo-light/MaterialContinuant> ,
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <https://w3id.org/gfo-light/hasMember> ;
                                                                 owl:someValuesFrom <https://w3id.org/gfo-light/MaterialContinuant>
                                                               ] ;
                                               rdfs:comment "The members of a material aggregate are regarded as its parts, similar to the parts of an object (e.g., John is part/member of the football team and plays the role of forward)."@en ;
                                               rdfs:label "Material aggregate"@en ;
                                               <http://www.w3.org/2004/02/skos/core#altLabel> "Collective"@en ,
                                                                                              "Group"@en ;
