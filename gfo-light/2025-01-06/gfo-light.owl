    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/hasOwner -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/hasOwner">
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/ownerOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/gfo-light/ImmaterialContinuant"/>
        <rdfs:range rdf:resource="https://w3id.org/gfo-light/MaterialContinuant"/>
        <rdfs:comment xml:lang="en">Immaterial continuants can be assigned to a corresponding material continuant using the object property hasOwner.</rdfs:comment>
        <rdfs:label xml:lang="en">has owner</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/hasPart -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/hasPart">
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/partOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/gfo-light/Individual"/>
        <rdfs:range rdf:resource="https://w3id.org/gfo-light/Individual"/>
        <rdfs:label xml:lang="en">has part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/hasParticipant -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/hasParticipant">
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/participatesIn"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Process"/>
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/gfo-light/Continuant"/>
        <rdfs:label xml:lang="en">has participant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/hasProcessPart -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/hasProcessPart">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/gfo-light/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/processPartOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/gfo-light/Process"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Attributive"/>
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Process"/>
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">has process part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/hasQuality -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/hasQuality">
