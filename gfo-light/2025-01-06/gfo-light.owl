    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/temporallyStarts -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/temporallyStarts">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/gfo-light/temporalPartOf"/>
        <rdfs:label xml:lang="en">temporally starts</rdfs:label>
        <skos:definition xml:lang="en">X starts Y: Xs = Ys and Xf &lt; Yf</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/timeOf -->


