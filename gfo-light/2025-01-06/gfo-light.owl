                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">has situation part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/hasTemporalPart -->


