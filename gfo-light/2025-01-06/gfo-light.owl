    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/memberOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/gfo-light/partOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/gfo-light/MaterialContinuant"/>
        <rdfs:range rdf:resource="https://w3id.org/gfo-light/MaterialAggregate"/>
        <rdfs:comment xml:lang="en">To model the membership of a member in a group/aggregate, the more specific object properties hasMember/memberOf should be used instead of hasPart/PartOf.</rdfs:comment>
        <rdfs:label xml:lang="en">member of</rdfs:label>
    </owl:ObjectProperty>
    


