            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/gfo-light/ImmaterialContinuant"/>
        <rdfs:label xml:lang="en">has boundary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/hasInnerBoundary -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/hasInnerBoundary">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/gfo-light/hasBoundary"/>
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/innerBoundaryOf"/>
        <rdfs:label xml:lang="en">has inner boundary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/hasMember -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/hasMember">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/gfo-light/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/memberOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/gfo-light/MaterialAggregate"/>
        <rdfs:range rdf:resource="https://w3id.org/gfo-light/MaterialContinuant"/>
        <rdfs:label xml:lang="en">has member</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/hasNaturalBoundary -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/hasNaturalBoundary">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/gfo-light/hasBoundary"/>
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/naturalBoundaryOf"/>
