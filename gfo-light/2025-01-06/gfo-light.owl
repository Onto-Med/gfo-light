        <rdfs:range rdf:resource="https://w3id.org/gfo-light/Entity"/>
        <rdfs:label xml:lang="en">instantiated by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/leadsTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/leadsTo">
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/resultsFrom"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Process"/>
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Process"/>
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">leads to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/locatedIn -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/locatedIn">
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/locationOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/MaterialObject"/>
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Process"/>
                    <rdf:Description rdf:about="https://w3id.org/gfo-light/Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/gfo-light/MaterialObject"/>
        <rdfs:comment xml:lang="en">Material objects can be located in other material objects (locatedIn, e.g., the driver is located in the car).</rdfs:comment>
        <rdfs:comment xml:lang="en">Processes and situations can take place (locatedIn) in material objects (e.g., a football match in a stadium or an illness in a person).</rdfs:comment>
        <rdfs:label xml:lang="en">located in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/gfo-light/locationOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/locationOf">
