    <!-- https://w3id.org/gfo-light/hasTime -->


    <owl:ObjectProperty rdf:about="https://w3id.org/gfo-light/hasTime">
        <owl:inverseOf rdf:resource="https://w3id.org/gfo-light/timeOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/gfo-light/ConcreteIndividual"/>
        <rdfs:range rdf:resource="https://w3id.org/gfo-light/TimeEntity"/>
