    <!-- https://w3id.org/gfo-light/CohesiveProcess -->


    <owl:Class rdf:about="https://w3id.org/gfo-light/CohesiveProcess">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gfo-core/Process"/>
        <rdfs:label xml:lang="en">Cohesive process</rdfs:label>
        <skos:altLabel xml:lang="en">Temporally connected process</skos:altLabel>
        <skos:definition xml:lang="en">A cohesive process is a causally and temporally connected process.</skos:definition>
        <skos:example xml:lang="en">A treatment of a patient in a hospital</skos:example>
        <skos:example xml:lang="en">Football match</skos:example>
        <skos:example xml:lang="en">The course of an illness</skos:example>
        <skos:example xml:lang="en">The first half of the match</skos:example>
    </owl:Class>
    


