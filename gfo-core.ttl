#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/gfo-core/after
:after rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :temporallyRelatedWith ;
       owl:inverseOf :before ;
       rdfs:label "after"@en .


###  https://w3id.org/gfo-core/attributiveOf
:attributiveOf rdf:type owl:ObjectProperty ;
               owl:inverseOf :hasAttributive ;
               rdfs:domain :Attributive ;
               rdfs:range :Individual ;
               rdfs:label "attributive of"@en .


###  https://w3id.org/gfo-core/before
:before rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :temporallyRelatedWith ;
        rdfs:label "before"@en ;
        skos:definition "X before Y: Xf < Ys"@en .


###  https://w3id.org/gfo-core/causedBy
:causedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :resultsFrom ;
          owl:inverseOf :causes ;
          rdfs:label "caused by"@en .


###  https://w3id.org/gfo-core/causes
:causes rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :leadsTo ;
        rdfs:label "causes"@en .


###  https://w3id.org/gfo-core/during
:during rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :temporalPartOf ;
        owl:inverseOf :temporallyContains ;
        rdfs:label "during"@en ;
        skos:definition "X during Y: Xs > Ys and Xf < Yf"@en .


###  https://w3id.org/gfo-core/hasAttributive
:hasAttributive rdf:type owl:ObjectProperty ;
                rdfs:domain :Individual ;
                rdfs:range :Attributive ;
                rdfs:label "has attributive"@en .


###  https://w3id.org/gfo-core/hasPart
:hasPart rdf:type owl:ObjectProperty ;
         owl:inverseOf :partOf ;
         rdfs:domain :Individual ;
         rdfs:range :Individual ;
         rdfs:label "has part"@en .


###  https://w3id.org/gfo-core/hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                owl:inverseOf :participatesIn ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Process
                                            :Situation
                                          )
                            ] ;
                rdfs:range :Continuant ;
                rdfs:label "has participant"@en .


###  https://w3id.org/gfo-core/hasProcessPart
:hasProcessPart rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasPart ;
                owl:inverseOf :processPartOf ;
                rdfs:domain :Process ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :Attributive
                                           :Process
                                           :Situation
                                         )
                           ] ;
                rdfs:label "has process part"@en .


###  https://w3id.org/gfo-core/hasSituationPart
:hasSituationPart rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasPart ;
                  owl:inverseOf :situationPartOf ;
                  rdfs:domain :Situation ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( :Attributive
                                             :Situation
                                           )
                             ] ;
                  rdfs:label "has situation part"@en .


###  https://w3id.org/gfo-core/hasTemporalPart
:hasTemporalPart rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :temporallyRelatedWith ;
                 owl:inverseOf :temporalPartOf ;
                 rdfs:label "has temporal part"@en .


###  https://w3id.org/gfo-core/hasTime
:hasTime rdf:type owl:ObjectProperty ;
         owl:inverseOf :timeOf ;
         rdfs:domain :ConcreteIndividual ;
         rdfs:range :TimeEntity ;
         rdfs:label "has time"@en .


###  https://w3id.org/gfo-core/instanceOf
:instanceOf rdf:type owl:ObjectProperty ;
            owl:inverseOf :instantiatedBy ;
            rdfs:domain :Entity ;
            rdfs:range :Category ;
            rdfs:label "instance of"@en .


###  https://w3id.org/gfo-core/instantiatedBy
:instantiatedBy rdf:type owl:ObjectProperty ;
                rdfs:domain :Category ;
                rdfs:range :Entity ;
                rdfs:label "instantiated by"@en .


###  https://w3id.org/gfo-core/leadsTo
:leadsTo rdf:type owl:ObjectProperty ;
         owl:inverseOf :resultsFrom ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Process
                                     :Situation
                                   )
                     ] ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :Process
                                    :Situation
                                  )
                    ] ;
         rdfs:label "leads to"@en .


###  https://w3id.org/gfo-core/partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain :Individual ;
        rdfs:range :Individual ;
        rdfs:label "part of"@en .


###  https://w3id.org/gfo-core/participatesIn
:participatesIn rdf:type owl:ObjectProperty ;
                rdfs:domain :Continuant ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :Process
                                           :Situation
                                         )
                           ] ;
                rdfs:label "participates in"@en .


###  https://w3id.org/gfo-core/processPartOf
:processPartOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :partOf ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Attributive
                                           :Process
                                           :Situation
                                         )
                           ] ;
               rdfs:range :Process ;
               rdfs:label "process part of"@en .


###  https://w3id.org/gfo-core/resultsFrom
:resultsFrom rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Process
                                         :Situation
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Process
                                        :Situation
                                      )
                        ] ;
             rdfs:label "results from"@en .


###  https://w3id.org/gfo-core/situationPartOf
:situationPartOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :partOf ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Attributive
                                             :Situation
                                           )
                             ] ;
                 rdfs:range :Situation ;
                 rdfs:label "situation part of"@en .


###  https://w3id.org/gfo-core/temporalPartOf
:temporalPartOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :temporallyRelatedWith ;
                rdfs:label "temporal part of"@en ;
                skos:definition "X temporal part of Y: X starts Y or X finishes Y or X during Y"@en .


###  https://w3id.org/gfo-core/temporallyContains
:temporallyContains rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasTemporalPart ;
                    rdfs:label "temporally contains"@en .


###  https://w3id.org/gfo-core/temporallyEqual
:temporallyEqual rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :temporallyRelatedWith ;
                 rdfs:label "temporally equal"@en ;
                 skos:definition "X equal Y: Xs = Ys and Xf = Yf"@en .


###  https://w3id.org/gfo-core/temporallyFinishedBy
:temporallyFinishedBy rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasTemporalPart ;
                      owl:inverseOf :temporallyFinishes ;
                      rdfs:label "temporally finished by"@en .


###  https://w3id.org/gfo-core/temporallyFinishes
:temporallyFinishes rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :temporalPartOf ;
                    rdfs:label "temporally finishes"@en ;
                    skos:definition "X finishes Y: Xs > Ys and Xf = Yf"@en .


###  https://w3id.org/gfo-core/temporallyMeets
:temporallyMeets rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :temporallyRelatedWith ;
                 owl:inverseOf :temporallyMetBy ;
                 rdfs:label "temporally meets"@en ;
                 skos:definition "X meets Y: Xf = Ys"@en .


###  https://w3id.org/gfo-core/temporallyMetBy
:temporallyMetBy rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :temporallyRelatedWith ;
                 rdfs:label "temporally met by"@en .


###  https://w3id.org/gfo-core/temporallyOverlappedBy
:temporallyOverlappedBy rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :temporallyRelatedWith ;
                        owl:inverseOf :temporallyOverlaps ;
                        rdfs:label "temporally overlapped by"@en .


###  https://w3id.org/gfo-core/temporallyOverlaps
:temporallyOverlaps rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :temporallyRelatedWith ;
                    rdfs:label "temporally overlaps"@en ;
                    skos:definition "X overlaps Y: Xs < Ys and Xf > Ys and Xf < Yf"@en .


###  https://w3id.org/gfo-core/temporallyRelatedWith
:temporallyRelatedWith rdf:type owl:ObjectProperty ;
                       rdfs:domain :Individual ;
                       rdfs:range :Individual ;
                       rdfs:comment "If a temporal relation is applied to other concrete individuals as time intervals (attributive, continuant, process, situation), it is propagated to their existence times (hasTime)."@en ,
                                    "In the definitions of the relations, the variables Xs, Xf, Ys and Yf represent the start (s) and finish (f) of the time intervals X and Y."@en ,
                                    "Some temporal relations can also be applied to points in time (before/after, equal) or a point in time and a time interval (before, starts, finishes, during or their inverse relations)."@en ;
                       rdfs:label "temporally related with"@en ;
                       skos:definition "Temporal relations between time intervals based on Allen, J. F. (1983). Maintaining knowledge about temporal intervals. Communications of the ACM, 26(11), 832-843."@en .


###  https://w3id.org/gfo-core/temporallyStartedBy
:temporallyStartedBy rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasTemporalPart ;
                     owl:inverseOf :temporallyStarts ;
                     rdfs:label "temporally started by"@en .


###  https://w3id.org/gfo-core/temporallyStarts
:temporallyStarts rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :temporalPartOf ;
                  rdfs:label "temporally starts"@en ;
                  skos:definition "X starts Y: Xs = Ys and Xf < Yf"@en .


###  https://w3id.org/gfo-core/timeOf
:timeOf rdf:type owl:ObjectProperty ;
        rdfs:domain :TimeEntity ;
        rdfs:range :ConcreteIndividual ;
        rdfs:label "time of"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/gfo-core/afterDateTimeValue
:afterDateTimeValue rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :dateTimeValue ;
                    rdfs:comment "Shortcut for 'after'"@en ;
                    rdfs:label "after date time value"@en .


###  https://w3id.org/gfo-core/beforeDateTimeValue
:beforeDateTimeValue rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :dateTimeValue ;
                     rdfs:comment "Shortcut for 'before'"@en ;
                     rdfs:label "before date time value"@en .


###  https://w3id.org/gfo-core/booleanValue
:booleanValue rdf:type owl:DatatypeProperty ;
              rdfs:domain :Attributive ;
              rdfs:range xsd:boolean ;
              rdfs:label "boolean value"@en .


###  https://w3id.org/gfo-core/containsDateTimeValue
:containsDateTimeValue rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :hasTemporalPartDateTimeValue ;
                       rdfs:comment "Shortcut for 'temporallyContains'"@en ;
                       rdfs:label "contains date time value"@en .


###  https://w3id.org/gfo-core/dateTimeValue
:dateTimeValue rdf:type owl:DatatypeProperty ;
               rdfs:domain :Individual ;
               rdfs:range xsd:dateTime ;
               rdfs:label "date time value"@en .


###  https://w3id.org/gfo-core/decimalValue
:decimalValue rdf:type owl:DatatypeProperty ;
              rdfs:domain :Attributive ;
              rdfs:range xsd:decimal ;
              rdfs:label "decimal value"@en .


###  https://w3id.org/gfo-core/finishedByDateTimeValue
:finishedByDateTimeValue rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf :hasTemporalPartDateTimeValue ;
                         rdfs:comment "Shortcut for 'temporallyFinishedBy'"@en ;
                         rdfs:label "finished by date time value"@en .


###  https://w3id.org/gfo-core/hasTemporalPartDateTimeValue
:hasTemporalPartDateTimeValue rdf:type owl:DatatypeProperty ;
                              rdfs:subPropertyOf :dateTimeValue ;
                              rdfs:comment "Shortcut for 'hasTemporalPart'"@en ;
                              rdfs:label "has temporal part date time value"@en .


###  https://w3id.org/gfo-core/startedByDateTimeValue
:startedByDateTimeValue rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf :hasTemporalPartDateTimeValue ;
                        rdfs:comment "Shortcut for 'temporallyStartedBy'"@en ;
                        rdfs:label "started by date time value"@en .


###  https://w3id.org/gfo-core/stringValue
:stringValue rdf:type owl:DatatypeProperty ;
             rdfs:domain :Attributive ;
             rdfs:range xsd:string ;
             rdfs:label "string value"@en .


###  https://w3id.org/gfo-core/unit
:unit rdf:type owl:DatatypeProperty ;
      rdfs:domain :Attributive ;
      rdfs:range xsd:string ;
      rdfs:label "unit"@en .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/gfo-core/Attributive
:Attributive rdf:type owl:Class ;
             rdfs:subClassOf :ConcreteIndividual ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :attributiveOf ;
                               owl:someValuesFrom :Individual
                             ] ;
             rdfs:comment "Qualities (attributes, traits, characteristics, etc.) of concrete individuals, relations (relators) between them and roles that objects can play in different contexts are subsumed under the category Attributive."@en ;
             rdfs:label "Attributive"@en ;
             skos:definition "Attributives are individuals that depend on other individuals by some kind of dependency relation."@en .


###  https://w3id.org/gfo-core/Category
:Category rdf:type owl:Class ;
          rdfs:subClassOf :Entity ;
          rdfs:label "Category"@en ;
          skos:altLabel "Class"@en ,
                        "Kind"@en ,
                        "Type"@en ;
          skos:definition "The notion of category covers all abstract entities that can be instantiated by or are predicated of other entities."@en .


###  https://w3id.org/gfo-core/Chronoid
:Chronoid rdf:type owl:Class ;
          rdfs:subClassOf :TimeEntity ,
                          [ rdf:type owl:Class ;
                            owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :temporallyFinishedBy ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onClass :TimeBoundary
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :temporallyStartedBy ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onClass :TimeBoundary
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ]
                                          [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :finishedByDateTimeValue ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onDataRange xsd:dateTime
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :startedByDateTimeValue ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onDataRange xsd:dateTime
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ]
                                        )
                          ] ;
          rdfs:label "Chronoid"@en ;
          skos:altLabel "Time interval"@en ,
                        "Time period"@en ;
          skos:definition "Chronoids are real-valued time intervals with endpoints."@en ;
          skos:example "The time (extension) of a football match (from the start point in time to the end point in time)"@en .


###  https://w3id.org/gfo-core/ConcreteIndividual
:ConcreteIndividual rdf:type owl:Class ;
                    rdfs:subClassOf :Individual ,
                                    [ rdf:type owl:Class ;
                                      owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                             owl:onProperty :finishedByDateTimeValue ;
                                                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                             owl:onDataRange xsd:dateTime
                                                                           ]
                                                                           [ rdf:type owl:Restriction ;
                                                                             owl:onProperty :startedByDateTimeValue ;
                                                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                             owl:onDataRange xsd:dateTime
                                                                           ]
                                                                         ) ;
                                                      rdf:type owl:Class
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasTime ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :TimeEntity
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :dateTimeValue ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange xsd:dateTime
                                                    ]
                                                  )
                                    ] ;
                    rdfs:label "Concrete individual"@en ;
                    skos:definition "Concrete individuals are entities that have an immediate relation to time or to space-time. They exist in time."@en .


###  https://w3id.org/gfo-core/Continuant
:Continuant rdf:type owl:Class ;
            rdfs:subClassOf :ConcreteIndividual ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                     owl:onProperty :finishedByDateTimeValue ;
                                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                     owl:onDataRange xsd:dateTime
                                                                   ]
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty :startedByDateTimeValue ;
                                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                     owl:onDataRange xsd:dateTime
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ]
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasTime ;
                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass :Chronoid
                                            ]
                                          )
                            ] ;
            rdfs:comment "Social or material (three-dimensional) objects, their groups/aggregates, parts and boundaries (two-, one- and zero-dimensional entities, i.e., surfaces, lines and points) are subsumed under the category Continuant."@en ,
                         "While the object parts are firmly connected to the entire object, the aggregate members are loose objects. When modelling a person, for example, you could consider their skeleton as an object and as part of the human body. However, if we look at bone finds in the context of anthropology, the skeleton can be modelled as an aggregate of individual bones. Another example is a computer. A kit of computer components is an object aggregate before assembly and an object (a computer) afterwards. However, if the components are thrown into the case without being correctly connected, it is not a computer, but the components are simply located in the case."@en ;
            rdfs:label "Continuant"@en ;
            skos:definition "Continuants are concrete individuals that persist through time and are wholly present at every point of their existence time (lifetime)."@en .


###  https://w3id.org/gfo-core/Entity
:Entity rdf:type owl:Class ;
        rdfs:label "Entity"@en ;
        skos:definition "Everything that exists is called an entity."@en .


###  https://w3id.org/gfo-core/Individual
:Individual rdf:type owl:Class ;
            rdfs:subClassOf :Entity ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :instanceOf ;
                              owl:someValuesFrom :Category
                            ] ;
            rdfs:label "Individual"@en ;
            skos:definition "Individuals are entities which cannot be further instantiated."@en .


###  https://w3id.org/gfo-core/Process
:Process rdf:type owl:Class ;
         rdfs:subClassOf :ConcreteIndividual ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :finishedByDateTimeValue ;
                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onDataRange xsd:dateTime
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :startedByDateTimeValue ;
                                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                  owl:onDataRange xsd:dateTime
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ]
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasTime ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :Chronoid
                                         ]
                                       )
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasParticipant ;
                           owl:someValuesFrom :Continuant
                         ] ,
                         [ rdf:type owl:Restriction ;
